let d1 = new Date(2017, 2, 11, 11, 30);
let changed = d1.toString();
let trim = changed.split(" ");
replacestrangecharacter(trim)
function replacestrangecharacter(strange) {
  var remove = strange.toString() 
      remove = remove.replace("(","")
      remove = remove.replace(")","")
      remove = remove.split(",");
      console.log(remove)
}
